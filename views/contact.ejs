<%- include('header.ejs')  %>


<br><br><br>
<div class="container-fluid">
    <div class="container">
        <div class="row">
            <h1 class="mt-5 text-center" style="color:#386576;">
                <span style=" border-bottom: 2px solid #386576;">Do Contact!</span>
            </h1>
            <div class="col-sm-2"></div>
            <div class="col-sm-8 mt-1">
                <form class="text-center">
                    <input type="text" name="name" class="form-control mt-4" placeholder="Name">
                    <input type="text" name="email" class="form-control mt-4" placeholder="email">
                    <input type="text" name="phone" class="form-control mt-4" placeholder="phone">
                    <textarea class="form-control mt-4" name="message" placeholder="Message"></textarea>
                    <button class="btn mt-4"
                        style="background:#fcc975; color: white; border:0; padding: 12px 26px;">Send
                        Message</button>
                </form>
               
            </div>
            <div class="col-sm-2"></div>
        </div><!--row end-->
    </div><!--container end-->
</div><!--container fluid end-->
<br>




<%- include('footer.ejs')  %>